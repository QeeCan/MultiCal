<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.qeecan.multical">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@drawable/mu"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/mu"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="n">
        <activity android:name=".Schedule.Alarm"></activity>
        <activity android:name=".Account.AccountEditActivity" />
        <activity android:name=".Note.NoteEditActivity" />
        <activity android:name=".Schedule.ScheduleEditActivity" />
        <activity android:name=".Constellation.LuckActivity" />
        <activity android:name=".Schedule.ScheduleActivity" />
        <activity android:name=".Account.AccountActivity" />
        <activity android:name=".Constellation.ConsActivity" />
        <activity android:name=".Note.RecActivity" />
        <activity android:name=".CalActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Schedule.notifaction.AlarmNotifaction" />
    </application>

</manifest>